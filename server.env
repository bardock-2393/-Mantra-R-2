# Server Environment Configuration
# This file configures the application to run everything locally on the server

# Environment
ENV=production
DEBUG=false

# Server Configuration
IS_REMOTE_SERVER=true
REMOTE_GPU_SERVER_URL=http://localhost:8001

# Ray Configuration (minimal for server)
RAY_NUM_CPUS=2
RAY_NUM_GPUS=0
RAY_OBJECT_STORE_MEMORY=500000000

# Processing Configuration
AUDIO_ENABLED=false
WINDOW_SECONDS=10

# Storage Configuration
LOCAL_UPLOAD_DIR=./static/uploads
DUCKDB_PATH=./data/events.duckdb

# Network Configuration
LOCAL_PORTS=8888 